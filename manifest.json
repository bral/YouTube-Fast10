{
  "manifest_version": 2,

  "name": "YouTube Fast10 ",
  "description": "Quickly search YouTube for videos",
  "version": "0.1",

  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["contentscript.js", "jquery-2.0.3.js"]
  }],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "background": {
    "scipts": ["backgroundscript.js"]
  },

  "permissions": ["<all_urls>", "contextMenus"]

}
